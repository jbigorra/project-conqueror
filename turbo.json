{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULTS", ".env*"],
      "outputs": ["dist/**", "dist/**/*.js", "dist/**/*.d.ts"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
